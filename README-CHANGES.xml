<c:changelog project="com.io7m.jorchard" xmlns:c="urn:com.io7m.changelog:4.0">
  <c:releases>
    <c:release date="2016-07-07T00:00:00+00:00" is-open="false" ticket-system="com.github.io7m.jorchard" version="0.1.0">
      <c:changes>
        <c:change date="2016-07-07T00:00:00+00:00" summary="Initial release."/>
      </c:changes>
    </c:release>
    <c:release date="2016-07-11T00:00:00+00:00" is-open="false" ticket-system="com.github.io7m.jorchard" version="0.2.0">
      <c:changes>
        <c:change date="2016-07-11T00:00:00+00:00" summary="Correct bug with setParent not correctly removing children."/>
        <c:change date="2016-07-11T00:00:00+00:00" summary="Make API fully exception safe: An exception never leaves the tree in an inconsistent state."/>
        <c:change date="2016-07-11T00:00:00+00:00" summary="Add the ability to prevent the detachment of nodes."/>
      </c:changes>
    </c:release>
    <c:release date="2016-11-23T00:00:00+00:00" is-open="false" ticket-system="com.github.io7m.jorchard" version="0.2.1">
      <c:changes>
        <c:change date="2016-11-23T00:00:00+00:00" summary="The published jar file is now an OSGi bundle."/>
        <c:change date="2016-12-03T00:00:00+00:00" summary="Add JOTreeNode.mapBreadthFirst() and JOTreeNode.mapDepthFirst()"/>
      </c:changes>
    </c:release>
    <c:release date="2016-12-30T00:00:00+00:00" is-open="false" ticket-system="com.github.io7m.jorchard" version="0.3.0">
      <c:changes>
        <c:change compatible="false" date="2016-12-30T00:00:00+00:00" summary="Change the API to return references to nodes instead of void.">
          <c:tickets>
            <c:ticket id="3"/>
          </c:tickets>
        </c:change>
      </c:changes>
    </c:release>
    <c:release date="2017-04-15T00:00:00+00:00" is-open="false" ticket-system="com.github.io7m.jorchard" version="0.3.1">
      <c:changes>
        <c:change compatible="false" date="2017-04-15T00:00:00+00:00" summary="Rename project. Use the new primogenitor POM and 2017 project conventions."/>
      </c:changes>
    </c:release>
    <c:release date="2017-12-08T00:00:00+00:00" is-open="false" ticket-system="com.github.io7m.jorchard" version="1.0.0">
      <c:changes>
        <c:change compatible="false" date="2017-12-08T00:00:00+00:00" summary="Publish JDK 9 modules, require JDK 9 to build and run"/>
        <c:change compatible="false" date="2017-12-08T00:00:00+00:00" summary="Remove jnull and jfunctional dependencies"/>
      </c:changes>
    </c:release>
